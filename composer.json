{
    "name": "dafiti/httpcache-listener",
    "description": "Dafiti HttpCache Response Listener for Silex Apps",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dafiti",
        "httpcache",
        "listener",
        "silex"
    ],
    "authors": [
        {
            "name": "Fabio da Silva Ribeiro",
            "email": "fabio.ribeiro@dafiti.com.br"
        }
    ],
    "support": {
        "issue": "https://github.com/dafiti/httpcache-listener/issues"
    },
    "minimum-stability": "dev",
    "suggest": {
        "dafiti/config-service-provider": "A simple way to manage config files in Silex"
    },
    "require": {
        "php": ">=5.5.0",
        "silex/silex": "~1.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "fabpot/php-cs-fixer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Dafiti\\Silex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dafiti\\Silex\\": "tests/"
        }
    }
}
